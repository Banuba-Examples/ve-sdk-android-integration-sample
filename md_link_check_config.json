{
    "ignorePatterns": [
        {
            "pattern": "10.0.4.(?:[4-9]|10)*."
        },
        {
            "pattern": "^(http|https)://localhost"
        },
        {
            "pattern": "https://github.com/google/ExoPlayer"
        },
        {
            "pattern": "https://github.com/bumptech/glide"
        },
        {
            "pattern": "https://github.com/Kotlin/kotlinx.coroutines"
        },
        {
            "pattern": "https://developer.android.com/jetpack/androidx"
        }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [200, 206]
}